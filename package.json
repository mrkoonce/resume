{
  "name": "resume",
  "version": "1.0.0",
  "description": "Resume",
  "main": "index.js",
  "type": "module",
  "config": {
    "components": "resources/assets/components",
    "pages": "src/pages"
  },
  "scripts": {
    "dev": "npm run development",
    "development": "npm run linting && vite build && npm run e8:dev && npm run pch:dev",
    "linting": "npm run eslint && npm run stylelint",
    "eslint": "eslint $npm_package_config_components $npm_package_config_pages",
    "stylelint": "stylelint \"$npm_package_config_pages/**/*.scss\"",
    "watch": "vite build --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrkoonce/resume.git"
  },
  "author": "Joseph Koonce",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/mrkoonce/resume/issues"
  },
  "homepage": "https://github.com/mrkoonce/resume#readme",
  "devDependencies": {
    "vite": "^6.2.0"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "eslint": "^9.21.0",
    "eslint-config-eslint": "^11.0.0",
    "glob": "^11.0.1",
    "stylelint": "^16.15.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-config-standard-scss": "^14.0.0"
  }
}
